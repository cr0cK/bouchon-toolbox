{
  "name": "mockify-toolbox",
  "version": "0.0.1",
  "description": "Toolbox for mockify",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "NODE_PATH=$NODE_PATH:./src mocha --compilers js:babel-core/register $(find . -name '*Spec.js')",
    "clean": "rm -rf dist",
    "build": "babel --ignore '*Spec.js' -d dist src",
    "release": "NODE_ENV=production npm run build",
    "prepublish": "npm test && npm run clean && npm run release"
  },
  "author": "Alexis MINEAUD <alexis.mineaud@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "lodash": "^3.10.1",
    "node-schema-object": "^2.0.2",
    "redux-act": "^0.2.0",
    "reselect": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.5",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^3.4.1",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.10.0",
    "mocha": "^2.3.4"
  }
}
